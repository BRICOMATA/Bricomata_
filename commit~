git remote add origin https://github.com/BRICOMATA/Bricomata_.git
git filter-branch --tree-filter 'rm -rf /home/aghiles/Aghiles/Ubuntu/conf/.thunderbird/4oyoecim.default/ImapMail/.lip6.fr/INBOX-1' HEAD
git push -u -f origin master
firefox https://github.com/BRICOMATA/Bricomata_.git
