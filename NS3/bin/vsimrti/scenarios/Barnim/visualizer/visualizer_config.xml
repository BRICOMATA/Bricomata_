<?xml version="1.0" encoding="UTF-8"?>
<!-- file version: 2013-11-26 -->
<configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:noNamespaceSchemaLocation="http://www.dcaiti.tu-berlin.de/research/simulation/download/get/scenarios/scenarioname/visualizer/visualizer_config.xsd">
    <visualizer id="filevisualizer" enabled="true" update="5" loader="com.dcaiti.vsimrti.fed.visualizer.FileVisualizerConfig">
        <filename>visualizer.csv</filename>
        <directory>.</directory>
        <separator>;</separator>
        <messages>
            <message id="VehicleMovements">
                <entries>
                    <entry>"MOVE_VEHICLE"</entry>
                    <entry>Time</entry>
                    <entry>Updated:Name</entry>
                    <entry>Updated:Speed</entry>
                    <entry>Updated:Heading</entry>
                    <entry>Updated:Position.Latitude</entry>
                    <entry>Updated:Position.Longitude</entry>
                    <entry>Updated:Position.Elevation</entry>
                    <entry>Updated:DistanceDriven</entry>
                    <entry>Updated:LongitudinalAcceleration</entry>
                    <entry>Updated:Slope</entry>
                    <entry>Updated:Stopped</entry>
                    <entry>Updated:RouteId</entry>
                    <entry>Updated:RoadPosition.EdgeId</entry>
                    <entry>Updated:RoadPosition.LaneIndex</entry>
                    <entry>Updated:VehicleEmissions.CurrentEmissions.Co2</entry>
                    <entry>Updated:VehicleEmissions.CurrentEmissions.Co</entry>
                    <entry>Updated:VehicleEmissions.CurrentEmissions.Hc</entry>
                    <entry>Updated:VehicleEmissions.CurrentEmissions.Pmx</entry>
                    <entry>Updated:VehicleEmissions.CurrentEmissions.Nox</entry>
                    <entry>Updated:VehicleConsumptions.CurrentConsumptions.Fuel</entry>
                    <entry>Updated:VehicleEmissions.AllEmissions.Co2</entry>
                    <entry>Updated:VehicleEmissions.AllEmissions.Co</entry>
                    <entry>Updated:VehicleEmissions.AllEmissions.Hc</entry>
                    <entry>Updated:VehicleEmissions.AllEmissions.Pmx</entry>
                    <entry>Updated:VehicleEmissions.AllEmissions.Nox</entry>
                    <entry>Updated:VehicleConsumptions.AllConsumptions.Fuel</entry>
                    <entry>Updated:VehicleSignals.BlinkerRight</entry>
                    <entry>Updated:VehicleSignals.BlinkerLeft</entry>
                    <entry>Updated:VehicleSignals.Brakelight</entry>
                </entries>
            </message>
            <message id="ReceiveV2XMessage">
                <entries>
                    <entry>"RECV_MESSAGE"</entry>
                    <entry>Time</entry>
                    <entry>Type</entry>
                    <entry>MessageId</entry>
                    <entry>ReceiverName</entry>
                    <entry>ReceiverInformation.ReceiveSignalStrength</entry>
                </entries>
            </message>
            <message id="SendV2XMessage">
                <entries>
                    <entry>"SEND_MESSAGE"</entry>
                    <entry>Time</entry>
                    <entry>Type</entry>
                    <entry>MessageId</entry>
                    <entry>SourceName</entry>
                    <entry>SourcePosition.Latitude</entry>
                    <entry>SourcePosition.Longitude</entry>
                    <entry>SourcePosition.Elevation</entry>
                </entries>
            </message>
            <message id="AddedVehicle" enabled="true">
                <entries>
                    <entry>"ADDED_VEHICLE"</entry>
                    <entry>Time</entry>
                    <entry>ApplicationVehicle.Name</entry>
                    <entry>ApplicationVehicle.Group</entry>
                    <entry>ApplicationVehicle.VehicleType.Length</entry>
                    <entry>ApplicationVehicle.VehicleType.MinGap</entry>
                    <entry>ApplicationVehicle.VehicleType.MaxSpeed</entry>
                    <entry>ApplicationVehicle.VehicleType.VehicleClass</entry>
                    <entry>ApplicationVehicle.VehicleType.Accel</entry>
                    <entry>ApplicationVehicle.VehicleType.Decel</entry>
                    <entry>ApplicationVehicle.VehicleType.Sigma</entry>
                    <entry>ApplicationVehicle.VehicleType.Tau</entry>
                    <entry>ApplicationVehicle.VehicleType.SpeedFactor</entry>
                    <entry>ApplicationVehicle.VehicleType.SpeedDeviation</entry>
                    <entry>RouteInfo.DepartingTime</entry>
                    <entry>RouteInfo.RouteId</entry>
                    <entry>RouteInfo.DepartureLane</entry>
                    <entry>RouteInfo.DeparturePos</entry>
                </entries>
            </message>
            <message id="AddedTrafficLight">
                <entries>
                    <entry>"ADDED_TRAFFICLIGHT"</entry>
                    <entry>Time</entry>
                    <entry>ApplicationTrafficLight.Name</entry>
                    <entry>ApplicationTrafficLight.Applications</entry>
                    <entry>TrafficLightGroup.FirstPosition.Latitude</entry>
                    <entry>TrafficLightGroup.FirstPosition.Longitude</entry>
                </entries>
            </message>
            <message id="AddedRsu">
                <entries>
                    <entry>"ADDED_RSU"</entry>
                    <entry>Time</entry>
                    <entry>ApplicationRsu.Name</entry>
                    <entry>ApplicationRsu.Group</entry>
                    <entry>ApplicationRsu.Applications</entry>
                    <entry>ApplicationRsu.Position.Latitude</entry>
                    <entry>ApplicationRsu.Position.Longitude</entry>
                    <entry>ApplicationRsu.Position.Elevation</entry>
                </entries>
            </message>
            <message id="ConfigureAdHocCommunication">
                <entries>
                    <entry>"CONFIGURE_ADHOC"</entry>
                    <entry>Time</entry>
                    <entry>Configuration.NodeID</entry>
                    <entry>Configuration.RadioMode</entry>
                    <entry>Configuration.Conf0.NewIP</entry>
                    <entry>Configuration.Conf0.NewPower</entry>
                    <entry>Configuration.Conf0.Channel0</entry>
                    <entry>Configuration.Conf0.Channel1</entry>
                    <entry>Configuration.Conf1.NewIP</entry>
                    <entry>Configuration.Conf1.NewPower</entry>
                    <entry>Configuration.Conf1.Channel0</entry>
                    <entry>Configuration.Conf1.Channel1</entry>
                </entries>
            </message>
            <message id="ConfigureCellCommunication">
                <entries>
                    <entry>"CONFIGURE_CELL"</entry>
                    <entry>Time</entry>
                    <entry>Configuration.NodeID</entry>
                    <entry>Configuration.Enabled</entry>
                    <entry>Configuration.MaxDlBitrate</entry>
                    <entry>Configuration.MaxUlBitrate</entry>
                </entries>
            </message>
            <message id="TrafficDetectorUpdates">
                <entries>
                    <entry>"DETECTOR_UPDATES"</entry>
                    <entry>Time</entry>
                    <entry>UpdatedInductionLoops:LaneId</entry>
                    <entry>UpdatedInductionLoops:VehicleCount</entry>
                    <entry>UpdatedInductionLoops:MeanSpeed</entry>
                    <entry>UpdatedInductionLoops:Occupancy</entry>
                    <entry>UpdatedLaneAreaDetectors:LaneId</entry>
                    <entry>UpdatedLaneAreaDetectors:Length</entry>
                    <entry>UpdatedLaneAreaDetectors:VehicleCount</entry>
                    <entry>UpdatedLaneAreaDetectors:MeanSpeed</entry>
                    <entry>UpdatedLaneAreaDetectors:Occupancy</entry>
                </entries>
            </message>
            <message id="ChangeLaneProperty">
                <entries>
                    <entry>"CHANGE_LANE_STATE"</entry>
                    <entry>Time</entry>
                    <entry>EdgeId</entry>
                    <entry>LaneIndex</entry>
                    <entry>AllowedVehicleClasses</entry>
                    <entry>DisallowedVehicleClasses</entry>
                    <entry>MaxSpeed</entry>
                </entries>
            </message>
        </messages>
    </visualizer>

    <visualizer id="itef" enabled="false" loader="com.dcaiti.vsimrti.fed.visualizer.ITEFVisualizerConfig">
        <upload enabled="false">
            <target></target>
            <scenarioId></scenarioId>
            <user></user>
            <password></password>
        </upload>
        <messages>
            <message id="AddedVehicle" enabled="true"/>
            <message id="VehicleMovements" enabled="true"/>
        </messages>
    </visualizer>

    <visualizer id="websocket" enabled="true" loader="com.dcaiti.vsimrti.fed.visualizer.WebsocketVisualizerConfig">
        <synchronized>true</synchronized>
        <port>46587</port>
        <messages>
            <message id="VehicleMovements" enabled="true"/>
            <message id="ReceiveV2XMessage" enabled="true"/>
            <message id="SendV2XMessage" enabled="true"/>
            <message id="AddedVehicle" enabled="true"/>
            <message id="AddedRsu" enabled="true"/>
        </messages>
    </visualizer>

</configuration>

