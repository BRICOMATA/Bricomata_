set kolmogorov smirnov critical values simulation
set goodness of fit fully specified off

let y1 = normal random numbers for i = 1 1 1000
let y2 = double expo rand numbers for i = 1 1 1000
let nu = 3
let y3 = t random numbers for i = 1 1 1000
let sigma = 1
let y4 = lognormal random numbers for i = 1 1 1000

set write decimals 5

let ksloc   = mean y1
let ksscale = sd   y1
normal kolmogorov-smirnov goodness of fit test y1
let ksloc   = mean y2
let ksscale = sd   y2
normal kolmogorov-smirnov goodness of fit test y2
let ksloc   = mean y3
let ksscale = sd   y3
normal kolmogorov-smirnov goodness of fit test y3
let ksloc   = mean y4
let ksscale = sd   y4
normal kolmogorov-smirnov goodness of fit test y4

